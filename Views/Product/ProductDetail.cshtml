@model Product
<br>

    <hr class="uk-divider-icon">
    <h3 class="uk-text-center">@Model.Name</h3>
    <p>
  
    </p>
    <div class="uk-container uk-position-center-right">
    <a class="uk-button uk-button-default"
       asp-controller="ShoppingCart"
       asp-action="AddToShoppingCart"
       asp-route-productId="@Model.ProductId">
        Add to Cart
    </a>

        @Model.Price
    </div>
<li>@Model.Description.</li>  
  @for (int i=0;i< Model.Properities.Count;i++)
            {
                <li>@Model.Properities[i]</li>
            }
    